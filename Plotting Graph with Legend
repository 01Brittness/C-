clear
clc

disp ('Plotting Graph with Legend');
disp ('--------------------------------------------');

x = [1:0.1:10];
z=(5*x.^1/4)+(4*x.^1/3)-(3*x.^1/2)+2*x-1;
y=(5*x.^4)+(4*x.^3)-(6*x.^2)+11*x-35;


plot(x,'b',y,'m*',z)
xlabel('X axis','color','b','fontsize',20);
ylabel('Y axis','color','m','fontsize',20);
title('Plot X, Y and Z','fontsize',20);
legend('(5*x.^4)+(4*x.^3)-(6*x.^2)+11*x-35','(5*x.^1/4)+(4*x.^1/4)+2*x-1')
grid on